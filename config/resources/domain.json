{
  "version": "0.1",
  "prefixes": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ext": "http://mu.semte.ch/vocabularies/ext/",
    "task": "http://redpencil.data.gift/vocabularies/tasks/"
  },
  "resources": {
    "vocabularies": {
      "name": "vocabulary",
      "class": "ext:VocabularyMeta",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "rdfs:label"
        },
        "alias": {
          "type": "string",
          "predicate": "dbpedia:alias"
        }
      },
      "relationships": {
        "source-datasets": {
          "predicate": "ext:sourceDataset",
          "target": "dataset",
          "cardinality": "many"
        },
        "mapping-shape": {
          "predicate": "ext:mappingShape",
          "target": "shacl-node-shape",
          "cardinality": "one"
        },
        "data-containers": {
          "predicate": "ext:content",
          "inverse": true,
          "target": "data-container",
          "cardinality": "many"
        }
      },
      "features": ["include-uri"],
      "new-resource-base": "https://my-application.com/vocabularies/"
    },
    "filter-count-inputs": {
      "name": "filter-count-input",
      "class": "ext:FilterCountInput",
      "attributes": {
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        },
        "source-class": {
          "type": "url",
          "predicate": "ext:sourceClass"
        },
        "source-path-string": {
          "type": "string",
          "predicate": "ext:sourcePathString"
        },
        "source-filter": {
          "type": "string",
          "predicate": "ext:sourceFilter"
        }
      },
      "relationships": {
        "dataset": {
          "predicate": "ext:dataset",
          "target": "dataset",
          "cardinality": "one"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://my-application.com/filter-count-input/"
    },
    "filter-count-outputs": {
      "name": "filter-count-output",
      "class": "ext:FilterCountOutput",
      "attributes": {
        "created": {
          "type": "datetime",
          "predicate": "dct:created"
        },
        "modified": {
          "type": "datetime",
          "predicate": "dct:modified"
        },
        "query": {
          "type": "string",
          "predicate": "ext:query"
        },
        "valid": {
          "type": "boolean",
          "predicate": "ext:valid"
        },
        "count": {
          "type": "integer",
          "predicate": "ext:count"
        },
        "warning": {
          "type": "string",
          "predicate": "ext:warning"
        },
        "error": {
          "type": "string",
          "predicate": "ext:error"
        }
      },
      "relationships": {
        "dataset": {
          "predicate": "ext:dataset",
          "target": "dataset",
          "cardinality": "one"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://my-application.com/filter-count-output/"
    }
  }
}
